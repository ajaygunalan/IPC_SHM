#include <math.h>
#include <stdio.h>
#include <stdlib.h>

int main()
{	unsigned int num[8];
	char* x[8];
	char temp[5];
	int i;
	printf("\n Stared..!");
	num[0]=01;
	num[1]=1;
	num[2]=126;
	num[3]=5678;
	num[4]=9101;
	num[5]=9999;
	num[6]=1314;
	num[7]=1516;
	
	for(i=0; i<8; i++)
	{
	printf("%d\n", i);
	printf("\nNum is %d", num[i]);
	sprintf(temp,"%d", num[i]);	
	x[i] = &temp;
	printf("\nString is %s \n", x[i]);
	printf("%d\n", i);
	}


return 0;
}
